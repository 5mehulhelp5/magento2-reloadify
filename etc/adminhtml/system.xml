<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="reloadify" translate="label" sortOrder="500" class="reloadify-extensions">
            <label>Reloadify</label>
        </tab>
        <section id="magmodules_reloadify" translate="label" type="text" sortOrder="1" showInDefault="1"
                 showInWebsite="1" showInStore="1">
            <label>General</label>
            <tab>reloadify</tab>
            <resource>Magmodules_Reloadify::config</resource>
            <include path="Magmodules_Reloadify::system/branding.xml"/>
            <include path="Magmodules_Reloadify::system/attributes.xml"/>
            <include path="Magmodules_Reloadify::system/general.xml"/>
            <include path="Magmodules_Reloadify::system/pwa.xml"/>
            <include path="Magmodules_Reloadify::system/debug.xml"/>
        </section>
    </system>
</config>

